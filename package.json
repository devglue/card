{
  "name": "@devglue/card",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-card-style": "theo ./design-specs/devglue-card.yml --format scss --dest ./design-specs/scss-output",
    "build-styles": "npm run build-card-style",
    "build-with-main-polyfills-and-scripts": "npm run build-styles && ng build card --prod --output-hashing=none --single-bundle true --keep-polyfills",
    "build-with-runtime-main-polyfills-styles-css-and-scripts": "npm run build-styles && ng build card --prod --output-hashing=none",
    "build-part": "npm run build-with-main-polyfills-and-scripts",
    "build-all": "npm run build-with-runtime-main-polyfills-styles-css-and-scripts",
    "run": "live-server ./dist/card",
    "build-and-run": "npm run build-all && npm run run"
  },
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devglue/card"
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/elements": "^7.2.5",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "ngx-build-plus": "^7.7.5",
    "rxjs": "~6.3.3",
    "theo": "github:salesforce-ux/theo",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
